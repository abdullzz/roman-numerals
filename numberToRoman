function numberToRoman(num){
    arr = [[1000,'M'],[900,'CM'],[500,'D'],[400,'CD'], [100,'C'],[90,'XC'],[50,'L'],[40,'XL'], [10,'X'],[9,'IX'],[5,'V'],[4,'IV'], [1,'I']]
    result = ''
    
    for(var x of arr){
        while(num >= x[0]){
            result += x[1]
            num -= x[0]
        }
    }
    
    return result
}
